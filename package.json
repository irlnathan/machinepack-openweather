{
  "name": "machinepack-openweather",
  "version": "1.0.0",
  "description": "Machines for communicating with the OpenWeather API from Node.js; e.g. to get current weather for a city, or for a specific latitude and longitude.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "OpenWeather",
    "weather",
    "conditions",
    "sails.js",
    "sails",
    "machines",
    "machinepack"
  ],
  "author": "Irl Nathan",
  "license": "MIT",
  "dependencies": {
    "machine": "^13.0.0-22",
    "machinepack-http": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:irlnathan/machinepack-openweather.git"
  },
  "devDependencies": {
    "test-machinepack-mocha": "^0.2.4"
  },
  "machinepack": {
    "friendlyName": "OpenWeather",
    "machineDir": "machines/",
    "machines": [
      "get-current-conditions"
    ],
    "testsUrl": "https://travis-ci.org/irlnathan/machinepack-openweather"
  }
}
